<svg width="800" height="600" xmlns="http://www.w3.org/2000/svg">
  <rect width="100%" height="100%" fill="#87CEEB"/>
  
  <!-- Ground -->
  <rect x="0" y="400" width="800" height="200" fill="#7CFC00"/>
  
  <!-- Picnic Table -->
  <rect x="500" y="350" width="200" height="80" fill="#8B4513" rx="5"/>
  <rect x="520" y="430" width="20" height="50" fill="#8B4513"/>
  <rect x="660" y="430" width="20" height="50" fill="#8B4513"/>
  <rect x="480" y="350" width="240" height="10" fill="#A52A2A"/>
  
  <!-- Picnic Blanket -->
  <rect x="150" y="420" width="250" height="150" fill="#FF6347" rx="10"/>
  
  <!-- Food Items -->
  <circle cx="250" cy="450" r="15" fill="#FFF" stroke="#000"/>
  <circle cx="280" cy="450" r="15" fill="#FFF" stroke="#000"/>
  <rect x="220" y="480" width="30" height="20" fill="#D2691E"/>
  <rect x="260" y="480" width="30" height="20" fill="#D2691E"/>
  <circle cx="300" cy="490" r="10" fill="#FF0000"/>
  <circle cx="320" cy="490" r="10" fill="#FF0000"/>
  
  <!-- Characters (simplified) -->
  <circle cx="200" cy="460" r="30" fill="#FFFF00" stroke="#000" stroke-width="2"/> <!-- Big Bird -->
  <circle cx="350" cy="470" r="20" fill="#FF0000" stroke="#000" stroke-width="2"/> <!-- Elmo -->
  <circle cx="250" cy="500" r="20" fill="#0000FF" stroke="#000" stroke-width="2"/> <!-- Cookie Monster -->
  <circle cx="550" cy="400" r="25" fill="#008000" stroke="#000" stroke-width="2"/> <!-- Oscar -->
  
  <!-- Trees -->
  <circle cx="700" cy="250" r="50" fill="#228B22" stroke="#000" stroke-width="2"/>
  <rect x="690" y="300" width="20" height="100" fill="#8B4513" stroke="#000" stroke-width="2"/>
  
  <circle cx="100" cy="250" r="50" fill="#228B22" stroke="#000" stroke-width="2"/>
  <rect x="90" y="300" width="20" height="100" fill="#8B4513" stroke="#000" stroke-width="2"/>
</svg>